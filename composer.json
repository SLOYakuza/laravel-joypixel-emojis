{
    "name": "sloyakuza/laravel-joypixel-emojis",
    "type": "library",
    "description": "Laravel helper for Joypixels emojis",
    "keywords": [
        "laravel",
        "emoji",
        "emojione",
        "joypixels",
        "SLOYakuza"
    ],
    "homepage": "https://github.com/SLOYakuza/laravel-joypixel-emojis",
    "license": "MIT",
    "authors": [
        {
            "name": "SLOYakuza",
            "email": "yakuza@rm-net.si",
            "homepage": "https://github.com/SLOYakuza",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "joypixels/emoji-toolkit": "^6",
        "illuminate/support": "^6|^7|^8|^9|^10"
    },
    "suggest": {
        "joypixels/assets": "If you want to serve the emojione assets yourself"
    },
    "autoload": {
        "psr-4": {
            "sloyakuza\\LaravelJoyPixels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sloyakuza\\LaravelJoyPixels\\": "tests"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "sloyakuza\\LaravelJoyPixels\\LaravelJoyPixelsServiceProvider"
            ],
            "aliases": {
                "LaravelJoyPixels": "sloyakuza\\LaravelJoyPixels\\LaravelJoyPixelsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
